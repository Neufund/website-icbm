            <div class="col-md-offset-2 col-md-5">
                <div class="number">
                    <%- sections[1].point -%>
                </div>
                <div class="content">
                    <h3><%- sections[1].title -%></h3>
                    <p><%- sections[1].paragraph1 %></p>
                    <p><%- sections[1].paragraph2 %></p>
                </div>
